<div class="row">
	<div class="col-md-8 col-md-offset-2 col-lg-6 col-lg-offset-3">
		<form id="form" class="panel panel-default" action="#">
			<div class="panel-heading">Informações do Perfil</div>
			<div class="panel-body pb-0">
				<div class="form-group">
					<label for="login">Login</label>
					<input id="login" name="login" class="form-control text-lowercase" type="text" spellcheck="false" readonly="readonly" value="<%=usuario.login %>" />
				</div>
				<div class="form-group">
					<label for="nome">Nome</label>
					<input id="nome" name="nome" maxlength="100" class="form-control" type="text" spellcheck="false" readonly="readonly" value="<%=usuario.nome %>" />
				</div>
				<div class="form-group">
					<label for="email">E-mail</label>
					<input id="email" name="email" maxlength="100" class="form-control" type="email" spellcheck="false" readonly="readonly" value="<%=usuario.email %>" />
				</div>
			</div>
		</form>
	</div>
</div>
